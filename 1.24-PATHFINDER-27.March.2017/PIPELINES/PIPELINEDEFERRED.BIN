   </Stage>

    <Stage id="DepthOfFieldBlur_ToneMap_4x"  enabled="false" >
      <ColourMask channels="RGBA"/>
      <BeginTarget target="FINAL_OR_DOF_TONEMAP" />
      <BindBuffer sampler="gBufferMap" sourceRT="HDRBUF_1" bufIndex="0" />

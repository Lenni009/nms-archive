      <BindBuffer sampler="gBuffer1Map" sourceRT="MOTION_MAXBUF" bufIndex="0" />
      <BindBuffer sampler="gBuffer2Map" sourceRT="MOTIONRESOLVE" bufIndex="0" />
      <BindBuffer sampler="gBuffer3Map" sourceRT="GBUFFER" bufIndex="1" />
      <BindBuffer sampler="gBuffer4Map" sourceRT="GBUFFER" bufIndex="0" />

      <SetContext zwrite="false" colourWrite="true" cullMode="back" blendMode="replace" depthTest="always" alphaToCoverage="false" stencilMode="maskRead" stencilRef="16" stencilMask="16"/>
      <DrawQuad material="materials/PostProcess.material.mbin" context="MBLUR_HIGH_PASS2" />

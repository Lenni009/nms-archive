      <DrawQuadMT material="materials/LensFlare.material.mbin" context="ANAMORPH_BG" compute="async_next" />
      <UnbindBuffers />
      <EndTarget flushCB="true" flushDB="false" />
      
      <BeginTarget target="LENS_ANABUF_B"/>

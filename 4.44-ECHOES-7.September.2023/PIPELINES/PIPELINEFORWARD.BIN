      <BindBuffer sampler="gBuffer3Map" sourceRT="BLOOM_FINALBUF"   bufIndex="0" />
      <BindBuffer sampler="gBuffer4Map" sourceRT="BLOOM_EXPBUF_B"   bufIndex="0" />
      <BindBuffer sampler="gBuffer5Map" sourceRT="LENSFLARE_BUF"    bufIndex="0" />
